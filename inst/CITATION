citHeader("To cite FunnelPlotR in publications use:")

citEntry(
  entry    = "Manual",
  title    = "FunnelPlotR: Funnel Plots for Comparing Institutional Performance",
  author   = "Chris Mainey",
  year     = "2022",
  note     = "Rpackage version 0.4.1",
  url      = "https://CRAN.R-project.org/package=FunnelPlotR",
  textVersion  =
  paste("Chris Mainey (2022).",
  "FunnelPlotR: Funnel Plots for Comparing Institutional Performance",
  "Rpackage version 0.4.1.",
  "https://CRAN.R-project.org/package=FunnelPlotR.")
)
